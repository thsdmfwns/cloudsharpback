{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "EnvironmentValues": [
    {
      "Key" : "CS_VOLUME_PATH",
      "Pattern" : "^\\/(?:[\\w.-]+\\/)*[\\w.-]+$\n",
      "DefaultValue" : "/var/cloud-sharp/"
    },
    {
      "Key" : "MYSQL_SERVER",
      "Pattern" : null,
      "DefaultValue" : "cs_db"
    },
    {
      "Key" : "MYSQL_PORT",
      "Pattern" : "^(0|[1-9][0-9]{0,3}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])$",
      "DefaultValue" : "3306"
    },
    {
      "Key" : "MYSQL_USER",
      "Pattern" : null,
      "DefaultValue" : null
    },
    {
      "Key" : "MYSQL_PASSWORD",
      "Pattern" : null,
      "DefaultValue" : null
    }
  ]
}
